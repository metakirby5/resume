section#experience
  +h4-i('Experience', 'fa-suitcase')
  div(data-bind="experience")
    a.card.uni-height(data-bind="container")
      .card-img(data-bind="image")
      .card-content
        .meta
          .name(data-bind="name")
          .role(data-bind="role")
          .date(data-bind="date")
        ul.main(data-bind="content")
          li(data-bind="value")
