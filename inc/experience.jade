section#experience
  +h4-i('Experience', 'fa-suitcase')
  .table-responsive
    table.table.table-super-condensed
      tbody(data-bind="experience")
        tr.active
          td: a(data-bind="name")
          td(data-bind="role")
          td(data-bind="date" align="right")
        tr
          td(colspan="3")
            ul.list-unstyled(data-bind="content")
              li(data-bind="value")